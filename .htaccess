Options All -Indexes

ErrorDocument 404 /404.html




# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress

RewriteEngine on
RewriteCond %{HTTP_HOST} ^vakulenkocvv.000webhostapp.com$
RewriteRule ^(.*)$ http://vakulenkocvv.pp.ua/$1 [R=301,L]